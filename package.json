{
  "name": "js-data-structure",
  "description": "JavaScript Data Structure",
  "version": "0.0.0-semantically-released",
  "main": "src/index.js",
  "scripts": {
    "build": "babel --copy-files --out-dir dist --ignore *.test.js lib-es6",
    "commit": "git-cz",
    "lint": "eslint --ignore-path .eslintignore -c .eslintrc .",
    "prebuild": "rimraf dist",
    "watch:test": "npm t -- -w",
    "test": "mocha --compilers js:babel-register --reporter spec",
    "cover": "nyc npm t",
    "start": "webpack-dev-server --content-base src --inline --hot",
    "validate": "npm ls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/almandsky/js-data-structure.git"
  },
  "keywords": [
    "JavaScript",
    "Data",
    "Structure"
  ],
  "author": "almandsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/almandsky/js-data-structure/issues"
  },
  "homepage": "https://github.com/almandsky/js-data-structure#readme",
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "^6.16.0",
    "babel-plugin-transform-class-properties": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-2": "6.18.0",
    "babel-register": "6.18.0",
    "commitizen": "2.8.6",
    "css-loader": "^0.25.0",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "^3.6.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.1.0",
    "ghooks": "^1.3.2",
    "mocha": "^2.5.3",
    "nyc": "8.4.0",
    "precommit-hook": "^3.0.0",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5"
  },
  "dependencies": {
    "chai": "^3.5.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "state-2"
    ]
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
